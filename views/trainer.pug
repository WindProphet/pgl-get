style(type="text/css").
  .battleteam-trainer-container {
    margin: 0 16px;
    position: relative;
    border: 1px solid rgba(153,153,153,.2);
    border-radius: 5px;
    box-shadow: 0 0 0 2px #FFF inset, 0 1px 0 rgba(0,0,0,.2);
  }
  .battleteam-trainer-container .battleteam-trainer-box .message {
    position: relative;
    display: table-cell;
    top: -60px;
    left: 390px;
    width: 490px;
    height: 48px;
    color: #333!important;
    padding: 2px 0;
    vertical-align: middle;
    background: #fff;
    line-height: 1.25em;
    word-break: break-all;
    font-size: 12px;
  }
  .trainerBox {
    position: relative;
    //- width: 365px;
    //- margin-right: 510px;
    height: 42px;
    padding: 8px;
    background: #ECECEC;
    border-radius: 2px;
    border: 2px solid #FFF;
    text-align: left;
    color: #333;
  }
  .trainerBox .trainerIcon {
    float: left;
    vertical-align: middle;
    width: 40px;
    height: 40px;
    border-radius: 2px;
    margin-right: 10px;
    margin: 0px;
    padding: 0px;
    border: 0px;
    font-weight: inherit;
    font-style: inherit;
    outline: none;
  }
  .trainerBox .countryCode {
    float: right;
    width: 47px;
    font-size: 11px;
    margin-top: 12px;
  }
  .trainerBox .countryCode:before {
    content: "";
    display: inline-block;
    width: 10px;
    height: 15px;
    margin-right: 4px;
    background: url(https://n-3ds-pgl-contents.pokemon-gl.com/pc/common/images/icon/common_icons.png) -17px -34px no-repeat;
    vertical-align: sub;
  }
  .trainerBox .savedataIdCode, .trainerBox .trainerName, .trainerBox .trainerNameRuby {
    margin-left: 50px;
    margin-right: 62px;
    line-height: 14px;
    white-space: nowrap;
  }
  .trainerBox .savedataIdCode {
    color: #999;
  }
  .trainerBox .savedataIdCode:before {
    content: "";
    display: inline-block;
    width: 16px;
    height: 11px;
    margin-right: 3px;
    background: url(https://n-3ds-pgl-contents.pokemon-gl.com/pc/common/images/icon/common_icons.png) 0 -34px no-repeat;
    vertical-align: text-top;
  }
mixin trainer(trainer)
  .trainerBox
    img.trainerIcon(src=("https://n-3ds1-pgl-trainericon.pokemon-gl.com/"+trainer.savedataId+".png"))
    .countryCode= trainer.countryCode
    .trainerName= trainer.trainerName
    .savedataIdCode= trainer.memberSavedataIdCode
